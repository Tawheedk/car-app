        <name>only show next 3 hours for endtime</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }

//Assuming the fields' variable names are 'start_time' and 'end_time'
//Adding an on change event listener to 'start_time' field

var startTimeField = g_form.getControl('starttime');
var endTimeField = g_form.getControl('endtime');
startTimeField.observe('change', function () {
    
    var startTime = startTimeField.getValue();
  
    // Get the timestamp of the selected start time in milliseconds
    var startTimestamp = Date.parseExact(startTime,"hh:mm tt");
  
    // Get the timestamp of the next 3 hours in milliseconds
    var endTimestamp = Date.now() + (3 * 60 * 60 * 1000);
  
    // Set the 3-hour limit on end time field
    var availableChoices = [];
    while (startTimestamp <= endTimestamp) {
        var time = new GlideDateTime(startTimestamp);
        availableChoices.push(time.getDisplayValue());
        startTimestamp += (15 * 60 * 1000); //increment by 15 mins
    }
    endTimeField.choices = availableChoices.join(',');
});
}]]></script>
