        <script><![CDATA[function onLoad() {
    var startField = g_form.getControl('x_980836_car_bookings.start');

    // Calculate the start time choices
    for (var minutes = 0; minutes < 1440; minutes += 15) {
        var seconds = minutes * 60;
        var gt = new GlideTime();
        gt.setValue(seconds);

        var label = gt.getByFormat('hh:mm a');
        var value = gt.getValue();

        addOption(startField, label, value);
    }

    // Add onchange event to start time field
    startField.onchange = function() {
        var start = g_form.getValue('x_980836_car_bookings.start');

        if (start) {
            var startSeconds = start * 60;
            var endSeconds = startSeconds + (3 * 60 * 60);

            var gt = new GlideTime();
            gt.setValue(endSeconds);

            g_form.setValue('x_980836_car_bookings.endtime', gt.getByFormat('hh:mm a'));
        }
    };
}

function addOption(selectElement, label, value) {
    var option = document.createElement('option');
    option.value = value;
    option.text = label;
    selectElement.appendChild(option);
}


]]></script>
