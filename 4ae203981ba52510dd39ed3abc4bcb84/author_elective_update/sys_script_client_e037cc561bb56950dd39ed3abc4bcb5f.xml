        <name>limitTime</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue === '') {
        return;
    }
    
    var startDate = g_form.getValue('starttime');
    var startHr = parseInt(startDate.split(':')[0]);
    var endHr = startHr + 3;
    
    var select = g_form.getControl('endtime');
    select.options.length = 0;
    
    for (var i = startHr; i <= endHr; i++) {
        if (i < 10) {
            var hrStr = '0' + i;
        } else {
            var hrStr = i.toString();
        }
        for (var j = 0; j < 60; j += 15) {
            var minStr = ('0' + j).slice(-2);
            var opt = new Option(hrStr + ':' + minStr, hrStr + ':' + minStr);
            select.options.add(opt);
        }
    }
}

var startField = g_form.getControl('starttime');
startField.observe('change', onChangeStartTime);]]></script>
