        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }

function onChangeStartTime() {
    var startField = g_form.getControl('starttime');
    var endField = g_form.getControl('endtime');

    var startTime = startField.value;
    var startTimeParts = startTime.split(":");
    var startHour = parseInt(startTimeParts[0]);
    var startMinute = parseInt(startTimeParts[1]);
    var endTimeOptions = endField.options;

    // Clear existing choices in end time field
    endTimeOptions.length = 0;

    // Set the default value of end time field to none
    var defaultOption = new Option('--None--', '');
    endTimeOptions.add(defaultOption);

    // Set the maximum end time as 3 hours from start time
    var maxEndHour = startHour + 3;
    var maxEndMinute = startMinute;

    // Generate choices for end time field
    for (var hour = startHour; hour <= maxEndHour; hour++) {
        var endHour = hour;
        var endMinute = maxEndMinute;
        var ampm = 'AM';
        if (endHour >= 12) {
            ampm = 'PM';
            if (endHour > 12) {
                endHour = endHour - 12;
            }
        }
        if (endHour === 0) {
            endHour = 12;
        }
        var endTime = endHour + ':' + ('0' + endMinute).slice(-2) + ' ' + ampm;
        var endTimeOption = new Option(endTime, endTime);
        endTimeOptions.add(endTimeOption);
    }
}

   
}

onChangeStartTime();]]></script>
