    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>only one booking allowed per day per user</description>
        <field>start_time</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>One Booking Per Day</name>
        <order/>
        <script><![CDATA[function onSubmit() {
   var startDateTime = g_form.getValue('start_time');
	var userid = g_form.getValue('userid');
	
	var booking = new GlideRecord('x_980836_car_bookings');
	booking.addQuery('userid', userid);
	booking.addQuery('start_time', '<=', startDateTime);
	booking.query();
	
	if (booking.hasNext()) {
		alert('You have already booked a parking spot for this time period.');
		return false;
	}

	return true;
}]]></script>
