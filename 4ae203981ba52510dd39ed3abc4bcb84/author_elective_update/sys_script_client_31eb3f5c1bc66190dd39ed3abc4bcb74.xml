        <messages/>
        <name>test part 2</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
  if (isLoading || newValue === '') {
    return;
  }

  var startTime = g_form.getValue('starttime');
  if (startTime) {
    var startDate = new Date();
    var startHour = parseInt(startTime.split(':')[0], 10);
    var startMinute = parseInt(startTime.split(':')[1].split(' ')[0], 10);
    var startAMPM = startTime.split(' ')[1];
    if (startAMPM === 'PM' && startHour !== 12) {
      startHour += 12;
    } else if (startAMPM === 'AM' && startHour === 12) {
      startHour = 0;
    }
    startDate.setHours(startHour);
    startDate.setMinutes(startMinute);
    startDate.setSeconds(0);
    startDate.setMilliseconds(0);

    var endDate = new Date(startDate.getTime() + 3 * 60 * 60 * 1000);
    var endHour = endDate.getHours();
    var endMinute = endDate.getMinutes();
    var endAMPM = endHour >= 12 ? 'PM' : 'AM';
    endHour = endHour % 12;
    endHour = endHour ? endHour : 12;
    var endTime = (endHour < 10 ? '0' + endHour : endHour) + ':' + (endMinute < 10 ? '0' + endMinute : endMinute) + ' ' + endAMPM;

    g_form.setValue('endtime', endTime);
  }
}
]]></script>
